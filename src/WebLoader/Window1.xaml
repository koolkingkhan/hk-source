<Window x:Class="WebLoader.MainWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel Background="Aqua">
        <TextBox Name="textBoxUrl" DockPanel.Dock="Top" Margin="3,3,3,3" />
        <DockPanel DockPanel.Dock="Bottom">
            <Button Content="EAP" Name="buttonEap2" HorizontalAlignment="Left" Width="75" Height="23" />
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Content="Thread" Name="buttonThread2" Width="75" Height="23" />
                <Button Content="TPL" HorizontalAlignment="Right" Name="buttonTPL2" Width="75" Height="23" />
            </StackPanel>
        </DockPanel>
        <WebBrowser DockPanel.Dock="Left" Margin="3,3,3,3" Name="webBrowser" />
    </DockPanel>
</Window>